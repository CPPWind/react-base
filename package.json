{
  "name": "ReactSeed",
  "version": "1.0.0",
  "author": "Pablo Magaz",
  "description": "ReactSeed",
  "main": "main.js",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^11.0.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "*",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "flux": "^2.1.1",
    "history": "^2.0.2",
    "install": "^0.6.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "*",
    "react": "^15.0.1",
    "react-addons-linked-state-mixin": "^15.0.1",
    "react-addons-test-utils": "~15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-react-router": "^1.0.0-beta3",
    "redux-router": "^1.0.0-beta8",
    "redux-slider-monitor": "^1.0.5",
    "redux-thunk": "^2.0.1",
    "scroll-behavior": "^0.4.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "scripts": {
    "logo": "node ./src/conf/.react-base/logo.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "lint": "npm run logo && node_modules/eslint/bin/eslint.js src",
    "test": "npm run logo && jest --coverage",
    "start": "npm run logo && node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 8000 --progress --colors",
    "postinstall": "npm run start"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testDirectoryName": "spec",
    "testFileExtensions": [
      "jsx"
    ],
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "scriptPreprocessor": "preprocessor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pmagaz/react-seed"
  },
  "keywords": [
    "ReactSeed"
  ],
  "license": "ISC"
}
