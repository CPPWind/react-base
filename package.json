{
  "name": "ReactBase",
  "version": "1.0.0",
  "author": "Pablo Magaz",
  "description": "React Base Isomorphic Redux App",
  "main": "client.js",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "*",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "cross-env": "^1.0.7",
    "css-modules-require-hook": "^4.0.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "install": "^0.7.3",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "*",
    "react": "^15.0.2",
    "react-addons-linked-state-mixin": "^15.0.2",
    "react-addons-test-utils": "~15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "yo": "*"
  },
  "scripts": {
    "logo": "node ./src/conf/.react-base/logo.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "build": "npm run logo && npm run clean && webpack --config webpack.config.babel.js",
    "clean": "rm dist/bundle.js || true",
    "lint": "npm run logo && node_modules/eslint/bin/eslint.js src",
    "test": "npm run logo && jest --coverage",
    "start": "cross-env NODE_ENV=development npm run logo && node server/",
    "start:component": "cross-env NODE_ENV=development npm run logo && node server/ --component",
    "start:prod": "cross-env NODE_ENV=production npm run build && node server/",
    "postinstall": "npm run start",
    "yo": "npm link src/conf/.react-base/templates/generator-react-base"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testDirectoryName": "spec",
    "testFileExtensions": [
      "jsx"
    ],
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "scriptPreprocessor": "preprocessor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atsistemas/react-base"
  },
  "keywords": [
    "ReactSeed"
  ],
  "license": "ISC"
}
