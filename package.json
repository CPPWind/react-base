{
  "name": "ReactBase",
  "version": "0.9.8",
  "description": "React Base Isomorphic Redux",
  "author": "Pablo Magaz",
  "contributors": [
    {
      "name": "Luis Martinez Marina",
      "email": "lmartinez.marina@atsistemas.com"
    },
    {
      "name": "Rafael Bernad",
      "email": "rbernad@atsistemas.com"
    },
    {
      "name": "Eduardo Perez",
      "email": "eperez-bermejo@atsistemas.com"
    }
  ],
  "main": "client/client.js",
  "scripts": {
    "logo": "node ./src/base/conf/.react-base/logo.js",
    "webpack": "node_modules/webpack/bin/webpack.js",
    "generate": "npm run logo && yo react-base",
    "generate:component": "npm run logo && yo react-base --option=2",
    "generate:container": "npm run logo && yo react-base --option=1",
    "regenerate:modelIndex": "node ./src/base/shared/Regenerate.js --model",
    "regenerate:routes": "node ./src/base/shared/Regenerate.js --routes",
    "regenerate:reducerIndex": "node ./src/base/shared/Regenerate.js --reducer",
    "regenerate": "npm run logo && node ./src/base/shared/Regenerate.js --all",
    "generate:dll": "webpack --config webpack/webpack.dll.config.babel.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.babel.js",
    "lint": "npm run logo && node node_modules/eslint/bin/eslint.js src --ignore-pattern **/*.spec.js  --ext .js --ext .jsx",
    "lint:css": "npm run logo && stylelint \"src/**/*.css\" --config .stylelintrc",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "npm run logo && mocha --compilers js:babel-core/register --recursive \"./src/**/*.spec.js\" --require ignore-styles \"./src/base/shared/JsDomSetup.js\"",
    "test:coverage": "nyc npm test && nyc report --reporter=lcov",
    "start": "npm run logo && cross-env NODE_ENV=development node server/",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server/",
    "postYeomanGenerator": "npm run regenerate && npm run start",
    "yo": "npm link src/base/conf/.react-base/templates/generator-react-base",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "postinstall": "npm run yo && npm run test:coverage && npm run generate:dll"
  },
  "dependencies": {
    "babel-core": "^6.14.0",
    "compression": "^1.6.2",
    "css-modules-require-hook": "^4.0.2",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-webpack-alias": "1.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^3.0.1",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "css-modules-require-hook": "^4.0.2",
    "enzyme": "^2.4.1",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "generator-webapp": "^2.1.0",
    "google-map-react": "^0.19.0",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "nyc": "7.1.0",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-modules-extract-imports": "^1.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.1.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-linked-state-mixin": "^15.3.1",
    "react-addons-test-utils": "~15.3.1",
    "react-controllables": "^0.6.0",
    "react-ink": "^5.1.1",
    "react-pure-render": "^1.0.2",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "supports-color": "^3.1.2",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.12.2",
    "yeoman-option-or-prompt": "^1.0.2",
    "yo": "^1.8.4"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "webpack/**"
    ],
    "extension": [
      ".jsx",
      ".es6",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atsistemas/react-base"
  },
  "bugs": {
